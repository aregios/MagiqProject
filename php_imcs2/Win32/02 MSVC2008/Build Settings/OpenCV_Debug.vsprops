<?xml version="1.0" encoding="windows-1253"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="OpenCV_Debug"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(OPENCV_DIR)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="opencv_core242d.lib opencv_imgproc242d.lib opencv_highgui242d.lib"
		AdditionalLibraryDirectories="$(OPENCV_DIR)\x86\vc9\lib"
	/>
</VisualStudioPropertySheet>
